# Environment Variables for DLD to Cursor AI Prompt Generation System

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# System Configuration
DEBUG=false
LOG_LEVEL=INFO
MAX_CONCURRENT_REQUESTS=10

# LLM Configuration
LLM_MODEL=gpt-4
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=4000

# Database Configuration (if using)
# DB_USER=dld_user
# DB_PASSWORD=secure_password
# DB_HOST=postgres
# DB_PORT=5432
# DB_NAME=dld_prompt_system

# Redis Configuration (if using)
# REDIS_HOST=redis
# REDIS_PORT=6379
# REDIS_DB=0

# Knowledge Base Configuration
KNOWLEDGE_BASE_PATH=knowledge_base/data
KNOWLEDGE_BASE_UPDATE_INTERVAL=3600

# Quality Thresholds
QUALITY_THRESHOLD_DLD_COMPLETENESS=0.8
QUALITY_THRESHOLD_TECHNICAL_ACCURACY=0.9
QUALITY_THRESHOLD_PROMPT_EFFECTIVENESS=0.85
QUALITY_THRESHOLD_CODE_QUALITY=0.9

# 5G Domain Configuration
DOMAIN_5G_STRICT_VALIDATION=false
DOMAIN_5G_ENABLE_ONTOLOGY=true

# Cursor AI Configuration
CURSOR_AI_PROMPT_FORMAT=structured
CURSOR_AI_MAX_PROMPT_LENGTH=8000
CURSOR_AI_OPTIMIZATION_LEVEL=high

# Performance Configuration
PERFORMANCE_MONITORING_ENABLED=true
PERFORMANCE_HISTORY_SIZE=1000
PERFORMANCE_ALERT_THRESHOLD=120  # seconds
